{
  "name": "thomasdominic/language-detector",
  "description": "Detect the language for your application using browser preferences, subdomains or route prefixes.",
  "keywords": ["Laravel", "Lumen", "Locale", "Language", "i18n"],
  "license": "MIT",
  "authors": [
    {
      "name": "Dominic THOMAS",
      "email": "dominic.thomas76@gmail.com"
    }
  ],
  "type": "package",
  "require": {
    "illuminate/config": "~5.0",
    "illuminate/translation": "~5.0",
    "illuminate/support": "~5.0",
    "illuminate/cookie": "~5.0"
  },
  "require-dev": {
    "fabpot/php-cs-fixer": "~1.11",
    "mockery/mockery": "0.9.*",
    "orchestra/testbench": "3.0.*",
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Thomasdominic\\LanguageDetector\\": "src/"
    },
    "files": ["src/Support/helpers.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Thomasdominic\\LanguageDetector\\Testing\\": "tests/"
    }
  },
  "config":{
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
